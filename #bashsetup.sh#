# Openai ocean development

ocean()
{
    cd /home/fischerq/openai/gym-ocean
}

ocean-parser()
{
    cd /home/fischerq/openai/gym-ocean/replay-transformer
}

clarity-analyzer()
{
    pushd /home/fischerq/Documents/clarity-analyzer
    java -jar target/analyzer.one-jar.jar
    popd
}

ocean_run_server()
{
    pushd /home/fischerq/openai/gym-ocean/utils/replay-transformer
    make
    cd java
    java -jar target/ocean.one-jar.jar > ../../../data/results/log.txt
    popd
}

ocean_parse_example()
{
    pushd /home/fischerq/openai/gym-ocean/utils/replay-transformer
    tools/parse_replay.sh ../../data/replays/2910472250.dem ../../data/parsed/2910472250.fullparse.pb
    popd
}

ocean_view_example()
{
    pushd /home/fischerq/openai/gym-ocean/utils/replay-transformer
    tools/interval-state-print.py ../../data/parsed/2910472250.fullparse.pb > ../../data/results/viewlog.txt
    popd
}

ocean_print_example()
{
    pushd /home/fischerq/openai/gym-ocean/utils/replay-transformer
    tools/display_parsed.py ../../data/parsed/2910472250.fullparse.pb > ../../data/results/example.txt
    popd
}

ocean_parse_test()
{
    pushd /home/fischerq/openai/gym-ocean/utils/replay-transformer
    tools/parse_replay_radiant.sh ../../data/statecheck/test.dem ../../data/statecheck/test.radiant.fullparse.pb 
    popd
}

ocean_copy_test()
{
    pushd /tmp/ocean
    DEM_FILE="$(ls -t *.dem | head -1)"
    PB_FILE="$(ls -t *.pb | head -1)"
    cp $DEM_FILE /home/fischerq/openai/gym-ocean/data/statecheck/test.dem
    cp $PB_FILE /home/fischerq/openai/gym-ocean/data/statecheck/test.radiant.direct.fullparse.pb
    popd
}

ocean_load_test()
{
    pushd /home/fischerq/openai/gym-ocean/utils/replay-transformer
    tools/interval-state-print.py ../../data/parsed/testgame_1.fullparse.pb > ../../data/results/printed_testgame_1.txt
    popd
}

ocean_compare_test()
{
    pushd /home/fischerq/openai/gym-ocean/utils/replay-transformer
    python tools/compare-replays.py ../../data/statecheck/test.radiant.fullparse.pb ../../data/statecheck/test.radiant.direct.fullparse.pb > ../../data/statecheck/comparison.txt
    popd
}

